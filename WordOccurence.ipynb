   "source": [
    "text = input(\"Enter a sentence: \")\n",
    "list1 = text.split()\n",
    "print(list1)\n",
    "\n",
    "wordcount = {}\n",
    "\n",
    "for word in list1:\n",
    "    word = word.lower()\n",
    "    if word in wordcount:\n",
    "        wordcount[word] += 1\n",
    "    else:\n",
    "        wordcount[word] = 1\n",
    "\n",
    "print(wordcount)"
   ]
